#  Sample configuration file
#  Copy to .config and edit

[general]
username = user@gmail.com
password = password
doc_name = Exact title of spreadsheet
doc_id = google_doc_id_from_url
csv_file = output.csv
json_file = output.json

# Specify some metadata for the JSON output
[dataset_metadata]
Title = Title of dataset
URL = http://url-of-dataset-page
License = CC BY-SA 4.0 International
License URI = http://creativecommons.org/licenses/by-sa/4.0/
Publisher = Your organisation or name
publisher email = foo@bar.org

# Specify some options
[options]
# selected_columns: Only output data from the specific columns
selected_columns = col1_title,col2_title,col3_title,name,email
# add_id_column: Add a sequential numbered ID column to aid linking of data
add_id_column = True

# Title map, rename the columns in the output
[title_map]
col1_title = My new title
col3_title = Another new title
